{
    "name": "plastyk/dashboard",
    "description": "An extendable dashboard for Silverstripe",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "dashboard"],
    "license": "MIT",
    "authors": [
        {
            "name": "3dgoo",
            "email": "michael@3dgoo.com"
        },
        {
            "name": "Turnerj",
            "email": "git@turnerj.com"
        },
        {
            "name": "Plastyk Studios",
            "email": "howdy@plastyk.com.au"
        }
    ],
    "require": {
        "silverstripe/admin": "^3.0",
        "silverstripe/framework": "^6.0"
    },
    "extra": {
        "installer-name": "dashboard",
        "expose": [
            "css",
            "javascript"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Plastyk\\Dashboard\\": "src/"
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs src/ tests/",
        "lint-clean": "vendor/bin/phpcbf src/ tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
